<section>
	<div class="container">
		<div class="row">
			<!-- Left sideBar -->
			{{left-sidebar categories=model.categories manufacturers=model.manufacturers extras=model.extras}}
			<!-- Left sideBar -->
			<div class="col-sm-9 padding-right"> 
				<div class="features_items">
                    <h2 class="title text-center">Tiendas y Sucursales</h2>
                    <div class="row">
                        {{#each model.offices as |office|}}
                        <script async defer
                        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAy4PzSIG77OtqMciXyhMuTD9tQSICh0LM&callback=initMap{{office.id}}">
                        </script>
                        <div class="col-lg-8">
                            <div id="map{{office.id}}" style="width: 550px; height: 400px;" data-lat="{{office.lat}}" data-lng="{{office.long}}"></div>
                            <br/>
                            <br/>
                        </div>
                        <div class="col-lg-4">
                            <h4>Nombre: </h4><p>{{office.name}}</p>
                            <br/>
                            <br/>
                            <h4>Ciudad: </h4><p>{{office.city}}</p>
                            <br/>
                            <br/>
                            <h4>Dirección: </h4><p>{{office.address}}</p>
                            <br/>
                            <br/>
                            <h4>Teléfono: </h4><p>{{office.phone}}</p>
                            <br/>
                            <br/>
                            <h4>Celular: </h4><p>{{office.cellphone}}</p>
                            <br/>
                            <br/>
                            <h4>WhatsApp: </h4><p>{{office.cellwsp}}</p>
                            <br/>
                            <br/>
                            <h4>Correo electrónico: </h4><p>{{office.email}}</p>
                            <img class="img-responsive" src="{{office.picture}}" alt="" />
                            <br/>
                            <br/>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
