{{outlet}}
<style type="text/css" media="all">
   h4,p {display: inline;}
</style>
<script>
    function initMap1() {
        var latitud = parseFloat(document.getElementById('map1').getAttribute("data-lat"));
        var longitud = parseFloat(document.getElementById('map1').getAttribute("data-lng"));
        var uluru = {lat: latitud, lng: longitud };
        var map = new google.maps.Map(document.getElementById('map1'), {
        zoom: 17,
        center: uluru
        });
        var marker = new google.maps.Marker({
        position: uluru,
        map: map
        });
    }
