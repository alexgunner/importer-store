{{outlet}}
<section>
	<div class="container">
		<div class="row">
			<!-- Left sideBar -->
			{{left-sidebar categories=model.categories manufacturers=model.manufacturers extras=model.extras}}
			<!-- Left sideBar -->
			<div class="col-sm-9 padding-right"> 
				<div class="features_items">
                <div class="row">
                <div class="col-lg-8">
                    <script>
                        function initMap() {
                            var latitud = parseFloat(document.getElementById('map').getAttribute("data-lat"));
                            var longitud = parseFloat(document.getElementById('map').getAttribute("data-lng"));
                            console.log(latitud);
                            var uluru = {lat: latitud, lng: longitud};
                            var map = new google.maps.Map(document.getElementById('map'), {
                            zoom: 17,
                            center: uluru
                            });
                            var marker = new google.maps.Marker({
                            position: uluru,
                            map: map
                            });
                        }
                    </script>
                <script async defer
                src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAy4PzSIG77OtqMciXyhMuTD9tQSICh0LM&callback=initMap">
                </script>
                <div class="col-lg-8">
                    <div id="map" style="width: 550px; height: 400px;" data-lat="{{model.contact.lastObject.lat}}" data-lng="{{model.contact.lastObject.long}}"></div>
                </div>
                </div>
                <div class="col-lg-4">
                    <h4>Dirección</h4>
                    <p>
                    {{model.contact.lastObject.address}}
                    </p>

                    <h4>Teléfonos</h4>
                    <p>
                    {{model.contact.lastObject.phone}}
                    </p>
                    <h4>Correo electrónico</h4>
                    <p>
                    {{model.contact.lastObject.email}}
                    </p>
                </div>
            </div>
                </div>
			</div>
        </div>
	</div>
</section>

